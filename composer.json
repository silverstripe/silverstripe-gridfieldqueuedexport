{
    "name": "silverstripe/gridfieldqueuedexport",
    "description": "Export large data sets from your GridField in the SilverStripe CMS interface through async jobs",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "queuedjobs",
        "export",
        "csv",
        "gridfield",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3 || ^8.0",
        "league/csv": "^8 || ^9",
        "silverstripe/framework": "^4.10",
        "symbiote/silverstripe-queuedjobs": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\GridfieldQueuedExport\\": "src/"
        }
    },
    "prefer-stable": true
}

