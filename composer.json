{
    "name": "silverstripe/gridfieldqueuedexport",
    "description": "Export large data sets from your GridField in the SilverStripe CMS interface through async jobs",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "queuedjobs",
        "export",
        "csv",
        "gridfield",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "require": {
        "league/csv": "^8",
        "silverstripe/framework": "^4",
        "symbiote/silverstripe-queuedjobs": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\GridfieldQueuedExport\\": "src/"
        }
    },
    "prefer-stable": true
}

